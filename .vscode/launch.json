    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
            "env": {},
            "args": [ "${input:dateArg}" ]
        },
        {
            "name": "Launch tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}"
        }
    ],
    "inputs": [
        {
            "id": "dateArg",
            "type": "promptString",
            "description": "Enter the input string",
            "default": "2019-03-17T00:00:00Z"
        }
    ]
}